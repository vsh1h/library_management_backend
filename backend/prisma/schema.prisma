generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url = env("DATABASE_URL")
}

model Book {
  id Int @id@default(autoincrement())
  title String
  summary String
  isbn String
  url String
}

model Author {
  id Int @id@default(autoincrement())
  first_name String
}